{
  "name": "ember-scrollmagic",
  "version": "1.0.0-beta3",
  "description": "An Ember addon for using ScrollMagic - the javascript library for magical scroll interactions.",
  "keywords": [
    "ember-addon",
    "scrollMagic",
    "scroll",
    "scrolling",
    "animation",
    "sticky",
    "pin",
    "fixed",
    "scrollbar",
    "scrub",
    "sync",
    "position",
    "progress",
    "parallax"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willviles/ember-scrollmagic.git"
  },
  "license": "MIT",
  "author": {
    "name": "Will Viles",
    "email": "will@vil.es",
    "url": "https://willviles.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://willviles.github.io/ember-scrollmagic",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout master"
  },
  "dependencies": {
    "broccoli-debug": "^0.6.3",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1",
    "fastboot-transform": "^0.1.2",
    "scrollmagic": "^2.0.5"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.10.1",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-document-title": "^0.4.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-fastboot": "2.1.1",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-head": "^0.4.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-markdown-resolver": "0.1.3",
    "ember-cli-sass": "~6.2.0",
    "ember-cli-sassdash": "^0.2.3",
    "ember-cli-showdown": "^4.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-prettify": "^0.0.1",
    "ember-composable-helpers": "^2.0.3",
    "ember-concurrency": "^1.0.0",
    "ember-config-service": "^1.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-gsap": "1.0.1",
    "ember-load-initializers": "^2.0.0",
    "ember-math-helpers": "^2.2.3",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-responsive": "^3.0.3",
    "ember-router-scroll": "^1.1.0",
    "ember-source": "^3.11.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^9.0.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.0"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://willviles.github.io/ember-scrollmagic/#/documentation/examples",
    "after": [
      "ember-gsap"
    ]
  }
}
