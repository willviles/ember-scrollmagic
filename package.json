{
  "name": "ember-scrollmagic",
  "version": "0.3.1",
  "description": "An Ember addon for using ScrollMagic - the javascript library for magical scroll interactions.",
  "keywords": [
    "ember-addon",
    "scrollMagic",
    "scroll",
    "scrolling",
    "animation",
    "sticky",
    "pin",
    "fixed",
    "scrollbar",
    "scrub",
    "sync",
    "position",
    "progress",
    "parallax"
  ],
  "license": "MIT",
  "author": "Will Viles <will@vil.es>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://willviles.github.io/ember-scrollmagic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willviles/ember-scrollmagic.git"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout master"
  },
  "dependencies": {
    "broccoli-debug": "^0.6.3",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "fastboot-transform": "^0.1.2",
    "scrollmagic": "^2.0.5"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.16.2",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-fastboot": "1.1.0",
    "ember-cli-github-pages": "^0.1.2",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-markdown-resolver": "0.1.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-sass": "~6.2.0",
    "ember-cli-sassdash": "^0.2.3",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-showdown": "^4.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-prettify": "^0.0.1",
    "ember-composable-helpers": "^2.0.3",
    "ember-concurrency": "^0.8.10",
