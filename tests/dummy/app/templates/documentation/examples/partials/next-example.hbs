{{#if nextExample}}
  {{#ui-section name="next-example" padding="md"}}
    <div class="container">
      <div class="row align-center">
        <div class="col xs12">
          <h4 class="font-header">Next Example...</h4>
          <br /><br />
          {{#ui-button-link-to 'documentation.examples.example' nextExample.slug
            bg="blue"
            color="white"
            padding="md"
            inline=true}}
            {{nextExample.title}}
          {{/ui-button-link-to}}
        </div>
      </div>
    </div>
  {{/ui-section}}
{{/if}}
